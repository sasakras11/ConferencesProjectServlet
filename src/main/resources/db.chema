create table Users(user_id INT AUTO_INCREMENT,username VARCHAR(50),password VARCHAR(200),
status VARCHAR(10),primary key (user_id));

create table Meetings(meeting_id INT AUTO_INCREMENT,name VARCHAR(100),suggested_name VARCHAR(100),
date VARCHAR(30),location VARCHAR(50),registered_people INT,visitedPeople INT,
speaker_id INT,primary key (meeting_id),FOREIGN KEY (meeting_id) REFERENCES Users(user_id));

create table user_id_to_meeting_id(id INT AUTO_INCREMENT,user_id INT,meeting_id INT,PRIMARY KEY (id),
FOREIGN KEY (meeting_id) REFERENCES Meetings(meeting_id),
FOREIGN KEY (user_id) REFERENCES Users(user_id));